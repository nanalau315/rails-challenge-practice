<h1>Edit <%= @building.name %></h1>

<%= form_for @building do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br>

<%= f.label :country %><br>
<%= f.text_field :country %><br>

<%= f.label :address %><br>
<%= f.text_field :address %><br>

<%= f.label :rent_per_floor %><br>
<%= f.text_field :rent_per_floor %><br>

<%= f.label :number_of_floors %><br>
<%= f.text_field :number_of_floors %><br>

<%= f.submit %>
<% end %>